#include <math.h>
int main()
{
  float a,b,c,delta,x1,x2;
  printf("A: ");
  scanf("%f", &a);
  printf("B: ");
  scanf("%f", &b);
  printf("C: ");
  scanf("%f", &c);
  
  delta = b*b-4*a*c;
  if(delta>=0){
      x1 = (-b+sqrt(delta))/(2*a);
      x2 = (-b-sqrt(delta))/(2*a);
     printf("%f", x1);
     printf("%f", x2);
  }else{
      printf("sem resultado");
  }
  
    return 0;
}
